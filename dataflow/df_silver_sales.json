{
	"name": "df_silver_sales",
	"properties": {
		"folder": {
			"name": "Silver"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_sales",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_sales",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "cast1",
					"description": "Convertir fechas a date."
				},
				{
					"name": "cast2",
					"description": "Convertir quantity a entero"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          sales_id as string,",
				"          sales_date as string,",
				"          store_id as string,",
				"          product_id as string,",
				"          quantity as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 derive(sales_date_str = toDate(replace(toString(sales_date), '/', '-'), 'dd-MM-yyyy')) ~> derivedColumn1",
				"derivedColumn1 cast(output(",
				"          sales_date_str as date 'yyyy-MM-dd'",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 cast(output(",
				"          quantity as integer '000'",
				"     ),",
				"     errors: true) ~> cast2",
				"cast2 select(mapColumn(",
				"          sales_id,",
				"          store_id,",
				"          product_id,",
				"          quantity,",
				"          sales_date = sales_date_str",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          sales_id as string,",
				"          sales_date as date,",
				"          store_id as string,",
				"          product_id as string,",
				"          quantity as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}